{
  "name": "@cronitorio/cronitor-rum-js",
  "version": "0.3.0",
  "description": "Official cronitor client (browser)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronitorio/cronitor-rum-js.git"
  },
  "keywords": [
    "cronitor",
    "analytics",
    "monitoring",
    "performance"
  ],
  "author": "Developers <support@cronitor.com>",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "yarn run build:targets && yarn run build:types",
    "build:targets": "node build.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "browserslist": "^4.21.5",
    "esbuild": "^0.17.12",
    "esbuild-plugin-browserslist": "^0.7.0",
    "prettier": "^2.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.5"
  },
  "bugs": {
    "url": "https://github.com/cronitorio/cronitor-rum-js/issues"
  },
  "homepage": "https://github.com/cronitorio/cronitor-rum-js#readme",
  "dependencies": {
    "web-vitals": "^3.3.0"
  }
}
